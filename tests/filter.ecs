import analysis
var df = analysis.read_csv("tests/cwurData.csv")
# system.out.println(df.find_broken_lines())
df = df.columns("country", "quality_of_education")
    .filter([](country, quality)->
        country == "USA" &&
        quality.to_number() <= 10
    )
df.select("institution", "quality_of_education").show()
df.aggregate(analysis.avg("score").as("score_avg")).show()
