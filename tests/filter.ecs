import analysis
var df = analysis.read_csv("cwurData.csv")
# system.out.println(df.find_broken_lines())
df.columns("country", "quality_of_education")
    .filter([](name, quality)->
        name == "USA" &&
        quality.to_number() <= 10
    )
    .select("institution")
    .to_csv("./result.csv")
