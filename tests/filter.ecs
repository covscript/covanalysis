import analysis as anal
var df = anal.read_csv("tests/cwurData.csv")
var tc = "USA", tq = 10
df = df.filter(
    anal::cond(
        country == tc &&
        quality_of_education.to_number() <= tq
    )
)
df.select(
    "institution",
    "quality_of_education"
).show()
df.aggregate(
    anal.avg("score").as("score_avg")
).show()
