import analysis
var df = analysis.read_csv("tests/cwurData.csv")
df = df.filter(
    analysis::cond(
        country == "USA" &&
        quality_of_education.to_number() <= 10
    )
)
df.select(
    "institution",
    "quality_of_education"
).show()
df.aggregate(
    analysis.avg("score").as("score_avg")
).show()
